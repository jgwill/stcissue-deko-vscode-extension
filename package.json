{
  "name": "stcissue-deko-vscode-extension",
  "version": "0.1.0",
  "description": "A VSCode extension for detecting '@STCIssue' in README.md",
  "main": "./out/extension.js",
  "publisher": "jgwill",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgwill/stcissue-deko-vscode-extension"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^4.3.5",
    "vscode": "^1.1.37",
    "tslint": "^5.12.1",
    "mocha": "^8.4.0",
    "@types/node": "^14.14.31",
    "@types/mocha": "^8.2.3"
  },
  "engines": {
    "vscode": "^1.58.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.checkReadme",
        "title": "Check README for '@STCIssue'"
      }
    ]
  }
}